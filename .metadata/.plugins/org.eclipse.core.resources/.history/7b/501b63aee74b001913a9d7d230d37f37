package baekjoon;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.ArrayDeque;
import java.util.Deque;

//AC
public class Q5430 {

	public static void main(String[] args) throws IOException {
		BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));
		int t = Integer.parseInt(reader.readLine());

		for(int i=0; i<t; i++) {
			String fx = reader.readLine();
			int n = Integer.parseInt(reader.readLine());
			if(n<=0) {
				System.out.println("error");
				continue;
			}
			String array = reader.readLine();
			array = array.substring(1,array.length()-1);
			String [] arrayNum = array.split(",");
			Deque<String> dq = new ArrayDeque<>();
			boolean error = false;

			for(String s : arrayNum)
				if(s.matches("[0-9]*"))
					dq.addLast(s);
			System.out.println(dq.toString()+", "+dq.size());
			for(int j=0; j<fx.length(); j++) {
				if(fx.substring(j,j+1).equals("D")) {
					if(dq.isEmpty()) {
						System.out.println("error");
						error = true;
					}else
						dq.pollFirst();
				}
				else {
					StringBuilder builder = new StringBuilder(dq.toString());
					String temp = builder.reverse().toString().trim();
					temp = temp.substring(1, temp.length()-1);
					String [] tempArray = temp.split(",");
					dq.clear();
					for(String s : tempArray)
						dq.addLast(s);
				}
			}
			if(!error)
				System.out.println(dq.toString().trim());
		}
	}
}
