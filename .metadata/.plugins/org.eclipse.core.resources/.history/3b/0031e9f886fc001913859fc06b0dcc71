package baekjoon.math;

import java.util.Scanner;

//a^b
public class Q10827 {

	public static void main(String [] args) {
		Scanner sc = new Scanner(System.in);
		double a = sc.nextDouble();
		double b = sc.nextDouble();
		
		double ans = a;
		for(int i=0; i<b-1; i++) {
			ans*=a;
		}
		System.out.println(a);
	}
}
