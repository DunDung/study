package baekjoon.divide_and_conquer;

import java.util.Arrays;
import java.util.Scanner;

//배열 합치기
public class Q11728 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt()+ scan.nextInt();
		int [] a = new int[n];
		for(int i=0; i<n; i++)
			a[i] = scan.nextInt();
		
		Arrays.sort(a);
		for(int k : a)
			System.out.print(k+" ");
	}

}
