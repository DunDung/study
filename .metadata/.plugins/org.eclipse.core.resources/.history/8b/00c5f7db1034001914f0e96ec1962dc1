package baekjoon;

import java.util.Arrays;
import java.util.Scanner;

public class Q8979 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int t=scan.nextInt();
		int goal=scan.nextInt();
		Olympic [] o = new Olympic[t];
		for(int i=0; i<t; i++) {
			int a = scan.nextInt();
			int b = scan.nextInt();
			int c = scan.nextInt();
			int d = scan.nextInt();
			o[i] = new Olympic(a, b, c, d);
		}
		
		Arrays.sort(o);
		System.out.println(o[goal-1].teamNo);
	
	}
}

class Olympic implements Comparable<Olympic>{
	int teamNo;
	int gold;
	int silver;
	int bronze;
	public Olympic(int a, int b,int c, int d) {
		this.teamNo = a;
		this.gold = b;
		this.silver = c;
		this.bronze = d;
	}
	@Override
	public int compareTo(Olympic o) {
		int r = this.gold-o.gold;
		if(r==0)
			r =this.silver-o.silver;
		if(r==0)
			r=this.bronze - o.bronze;
		return r;
	}
}