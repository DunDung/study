package baekjoon;

import java.util.Scanner;

public class Q2935 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		long a = scan.nextLong();
		String s = scan.next();
		long b = scan.nextLong();
		if(s.equals("+"))
			System.out.println(a+b);
		else
			System.out.println(a*b);
	}

}
