package baekjoon.implementation;

import java.util.Scanner;

//소용돌이 예쁘게 출력하기
public class Q1022 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int r1 = scan.nextInt();
		int c1 = scan.nextInt();
		int r2 = scan.nextInt();
		int c2 = scan.nextInt();
		int n = r2-r1;
		int m = c2-c1;
		System.out.println(n+" "+m);
	}

}
